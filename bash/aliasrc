alias vim='/usr/bin/nvim'
alias ll='ls -l --color'
alias la='ls -Al --color'
alias tr='tree'
alias tm='tmux new -s h@ck -n main'
alias emacs='emacsclient --create-frame --alternate-editor=""'
alias ytp='yt-dlp --yes-playlist --skip-unavailable-fragments -x --audio-format aac --audio-quality 0'

alias gs='git status'
alias gc='git commit -m'
alias ga='git add'
